{
  "expo": {
    "name": "Find a Spot!",
    "slug": "FindASpot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/FASIcon.png",
    "splash": {
      "image": "./src/images/FASIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/images/FASwelcome.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mariekiecoetzee.FindASpot"
    },
    "web": {
      "favicon": "./src/images/FASIcon.png"
    }
  }
}
